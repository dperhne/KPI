      DTS:Expression="&quot;SELECT A.KPICode, A.DeptID, ABS(A.KPIValue-B.KPIValue)/B.KPIValue AS Percent_Diff&#xA;  FROM [lsa_AdminData].[dbo].[KPIData_Stage] A, [lsa_AdminData].[dbo].[KPIData] B&#xA;  WHERE A.DeptID = B.DeptID&#xA;  and A.KPICode = B.KPICode&#xA;  and A.AcadYear = '&quot; + @_sAcadYear + &quot;'&#xA;  and B.AcadYear = (A.AcadYear-1) &#xA;  and B.KPIValue &gt; 0&#xA;&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="vKPI_Verification">
      <DTS:VariableValue
        DTS:DataType="8">SELECT A.KPICode, A.DeptID, ABS(A.KPIValue-B.KPIValue)/B.KPIValue AS Percent_Diff
  FROM [lsa_AdminData].[dbo].[KPIData_Stage] A, [lsa_AdminData].[dbo].[KPIData] B
  WHERE A.DeptID = B.DeptID
  and A.KPICode = B.KPICode
  and A.AcadYear = '2017'
  and B.AcadYear = (A.AcadYear-1) 
  and B.KPIValue &gt; 0
</DTS:VariableValue>
    </DTS:Variable>
